root ::= char

char ::= "[character(" string ")" ws (
"{"
"Nickname(" string ")" ws
"Species(" string ")" ws
"Age(" list ")" ws
"Features(" list ")" ws
"Body(" list ")" ws
"Mind(" list ")" ws
"Personality(" list ")" ws
"Loves(" list ")" ws
"Hates(" list ")" ws
"Description(" list ")" ws
"}]" )

list ::= string (" + " string)*

# deliberately restrict characters to force lists to use + instead of commas, periods or semicolons.
string ::= "\"" [ 0-9a-zA-Z]+ "\""

ws ::= ([ \t\n] ws)?
